{
  "name": "amazeui-vue",
  "version": "0.0.1",
  "description": "amazeui elements for vue.js",
  "keywords": [
    "component",
    "amazeui",
    "vue",
    "mobile"
  ],
  "main": "lib/amazeui-vue.common.js",
  "style": "lib/style.css",
  "files": [
    "lib",
    "src",
    "packages"
  ],
  "scripts": {
    "_NOTE": "生成入口文件，生成UMD CJS, 组件",
    "bootstrap": "yarn || npm i",
    "build:file": "npm run lint && node build/bin/build-entry",
    "demo:dev": "npm run bootstrap && npm run build:file && webpack-dev-server --config build/webpack.demo.js --inline --hot --port 8000 -p",
    "demo:build": "npm run bootstrap && npm run build:file && cross-env BUILD_ENV=DEMO node build/bin/build-webpack.js",
    "demo:start": "node ./examples/server.js",
    "dist": "npm run clean && npm run build:file && cross-env BUILD_ENV=UMD node build/bin/build-webpack.js && cross-env BUILD_ENV=COMMONJS node build/bin/build-webpack.js && cross-env BUILD_ENV=COMPONENT node build/bin/build-webpack.js",
    "dist:all": "node build/bin/build-all.js",
    "deploy": "build:file && node build/cooking.demo.js -p && gh-pages -d example/dist --remote amazeui",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "lint": "eslint src/**/*.js packages/**/*.{js,vue} build/**/*.js --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangchy/amazeUI-vue.git"
  },
  "homepage": "https://github.com/zhangchy/amazeUI-vue.git#readme",
  "bugs": {
    "url": "https://github.com/zhangchy/amazeUI-vue.git/issues"
  },
  "author": "elemefe",
  "license": "MIT",
  "dependencies": {
    "array-find-index": "^1.0.2",
    "raf.js": "0.0.4",
    "vue": "^2.4.2",
    "vue-lazyload": "^1.0.1"
  },
  "peerDependencies": {
    "vue": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^2.1.0",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.30.1",
    "json-templater": "^1.1.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "my-local-ip": "^1.0.0",
    "node-sass": "^4.5.3",
    "ora": "^1.3.0",
    "postcss-loader": "^2.0.6",
    "postcss-px2rem": "^0.3.0",
    "rimraf": "^2.6.1",
    "sass": "^1.0.0-beta.2",
    "sass-loader": "^6.0.6",
    "shell": "^0.3.2",
    "style-loader": "^0.18.2",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.4",
    "vue-router": "^2.7.0",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  }
}
